{
  "name": "react-render-hook",
  "version": "0.1.3",
  "description": "Hooks to React.js rendering, to enable access to the virtual DOM (as shown in React devtools)",
  "scripts": {
    "build": "babel src -d lib --source-maps true",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-register --compilers jsx:babel-register src/tests/**/*.spec.js"
  },
  "main": "lib/globalHook.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bruderstein/react-render-hook.git"
  },
  "author": "Dave Brotherstone <davegb@pobox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bruderstein/react-render-hook/issues"
  },
  "homepage": "https://github.com/bruderstein/react-render-hook",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "jsdom": "^6.5.1",
    "magicpen": "^5.4.0",
    "mocha": "^2.3.3",
    "object-assign": "^4.0.1",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "unexpected": "^10.0.2"
  },
  "files": [
    "lib/*.js",
    "lib/react-devtools/backend/*.js",
    "README.md",
    "src/*.js",
    "src/react-devtools/backend/*.js"
  ]
