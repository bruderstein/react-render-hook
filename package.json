{
  "name": "react-render-hook",
  "version": "2.0.0",
  "description": "Hooks to React.js rendering, to enable access to the virtual DOM (as shown in React devtools)",
  "scripts": {
    "build": "babel src -d lib --source-maps true --config-file ./.babelrc",
    "prepare": "npm run build",
    "test": "mocha --require babel-require.js src/tests/**/*.spec.js"
  },
  "main": "lib/globalHook.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bruderstein/react-render-hook.git"
  },
  "author": "Dave Brotherstone <davegb@pobox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bruderstein/react-render-hook/issues"
  },
  "homepage": "https://github.com/bruderstein/react-render-hook",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "create-react-class": "^15.6.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "magicpen": "^5.4.0",
    "mocha": "^6.2.2",
    "object-assign": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "unexpected": "^10.0.2"
  },
  "files": [
    "lib/*.js",
    "lib/react-devtools/backend/*.js",
    "README.md",
    "src/*.js",
    "src/react-devtools/backend/*.js"
  ],
  "dependencies": {
    "fbjs": "^1.0.0",
    "semver": "^5.5.1"
  }
}
